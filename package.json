    "body-parser": "^1.19.0",
    "chai-things": "^0.2.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "crypo": "^0.1.4",
    "debug": "~2.6.9",
    "ejs": "~2.6.1",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "install": "^0.13.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "mongodb": "^3.3.3",
    "mongoose": "^5.7.7",
    "morgan": "~1.9.1",
    "npm": "^6.12.1"
