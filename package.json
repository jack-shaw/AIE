    "@babel/register": "^7.0.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "chai-things": "^0.2.0",
    "cross-env": "^6.0.3",
    "eslint": "^6.6.0",
    "eslint-watch": "^6.0.1",
